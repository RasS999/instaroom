{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/views/(.*)",
      "dest": "/views/$1"
    },
    {
      "src": "/libs/(.*)",
      "dest": "/libs/$1"
    },
    {
      "src": "/(.*)",
      "dest": "app.js"
    }
  ]
}
